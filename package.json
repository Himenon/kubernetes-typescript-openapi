{
  "name": "@himenon/kubernetes-typescript-openapi",
  "version": "1.4.0",
  "description": "Implementation of TypeScript generated from Kubernetes OpenAPI",
  "keywords": [
    "Kubernetes",
    "k8s",
    "OpenAPI",
    "Swagger"
  ],
  "homepage": "https://github.com/Himenon/kubernetes-typescript-openapi#readme",
  "bugs": {
    "url": "https://github.com/Himenon/kubernetes-typescript-openapi/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Himenon/kubernetes-typescript-openapi.git"
  },
  "license": "MIT",
  "author": {
    "name": "Himenon",
    "email": "k.himeno314@gmail.com",
    "url": "https://github.com/Himenon"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./lib/$cjs/index.js",
  "module": "./lib/$esm/index.js",
  "types": "./lib/$types/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "pnpm ts ./scripts/build.ts",
    "build:all": "run-s fetch:schemas generate:code",
    "clean": "pnpm ts ./scripts/clean.ts",
    "fetch:schemas": "pnpm ts ./scripts/fetchSchema.ts",
    "format:code": "run-s prettier:format eslint:format",
    "format:yarn:lock": "yarn-deduplicate yarn.lock --strategy highest",
    "lerna:version:up": "lerna version --yes",
    "generate:code": "pnpm ts ./scripts/generateCode.ts",
    "release:github:registry": "pnpm publish --registry https://npm.pkg.github.com/:_authToken=${NODE_AUTH_TOKEN}",
    "release:npm:registry": "pnpm publish",
    "test": "run-s depcruise:validate eslint:validate prettier:validate",
    "prettier:format": "prettier \"**/*.{js,jsx,ts,tsx,json,yml,yaml,md,html}\" --write ./src",
    "prettier:validate": "prettier --check \"**/*.{js,jsx,ts,tsx,json,yml,yaml,md,html}\"",
    "eslint:format": "eslint \"**/*.{ts,tsx}\" --fix",
    "eslint:validate": "eslint \"**/*.{ts,tsx}\"",
    "depcruise:validate": "depcruise --validate .dependency-cruiser.js src",
    "ts": "node --no-warnings=ExperimentalWarning --experimental-specifier-resolution=node --loader ts-node/esm"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "devDependencies": {
    "@commitlint/cli": "18.6.0",
    "@commitlint/config-conventional": "18.6.0",
    "@himenon/openapi-typescript-code-generator": "1.0.2",
    "@swc/core": "^1.3.107",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.16",
    "@types/node-fetch": "2.6.11",
    "@types/rimraf": "3.0.2",
    "@typescript-eslint/eslint-plugin": "6.20.0",
    "@typescript-eslint/parser": "6.20.0",
    "cherry-pick": "^0.5.0",
    "conventional-changelog-angular-all": "1.7.0",
    "cpy": "11.0.0",
    "dependency-cruiser": "16.1.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "execa": "8.0.1",
    "generate-changelog": "1.8.0",
    "jest": "29.7.0",
    "jest-cli": "29.7.0",
    "lerna": "8.0.2",
    "lint-staged": "15.2.1",
    "node-fetch": "^3.3.2",
    "npm-run-all": "4.1.5",
    "prettier": "3.2.4",
    "rimraf": "5.0.5",
    "simple-git-hooks": "^2.9.0",
    "sort-package-json": "2.7.0",
    "swagger2openapi": "^7.0.8",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "yarn-deduplicate": "6.0.2"
  },
  "engines": {
    "pnpm": ">=6"
  },
  "publishConfig": {
    "access": "public"
  }
}
