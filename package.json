{
  "name": "@himenon/kubernetes-typescript-openapi",
  "version": "2.1.0",
  "description": "Implementation of TypeScript generated from Kubernetes OpenAPI",
  "keywords": [
    "Kubernetes",
    "k8s",
    "OpenAPI",
    "Swagger"
  ],
  "homepage": "https://github.com/Himenon/kubernetes-typescript-openapi#readme",
  "bugs": {
    "url": "https://github.com/Himenon/kubernetes-typescript-openapi/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Himenon/kubernetes-typescript-openapi.git"
  },
  "license": "MIT",
  "author": {
    "name": "Himenon",
    "email": "k.himeno314@gmail.com",
    "url": "https://github.com/Himenon"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./lib/$cjs/index.js",
  "module": "./lib/$esm/index.js",
  "types": "./lib/$types/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "build:all": "run-s fetch:schemas generate:code",
    "clean": "rimraf dist",
    "fetch:schemas": "pnpm ts ./scripts/fetchSchema.ts",
    "format:unsafe": "biome check --apply-unsafe",
    "lerna:version:up": "lerna version --yes",
    "generate:code": "pnpm ts ./scripts/generateCode.ts",
    "release:github:registry": "pnpm publish --registry https://npm.pkg.github.com/:_authToken=${NODE_AUTH_TOKEN}",
    "release:npm:registry": "pnpm publish",
    "test": "run-s validate",
    "format": "biome format --write .",
    "validate": "biome check --apply .",
    "ts": "node --no-warnings=ExperimentalWarning --experimental-specifier-resolution=node --loader ts-node/esm"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,json,yml,yaml,html,md,ts,tsx}": [
      "biome format --no-errors-on-unmatched --write"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@himenon/openapi-typescript-code-generator": "1.0.2",
    "@swc/core": "^1.3.107",
    "@types/node": "20.11.16",
    "@typescript-eslint/eslint-plugin": "6.20.0",
    "@typescript-eslint/parser": "6.20.0",
    "conventional-changelog-angular-all": "1.7.0",
    "cross-env": "^7.0.3",
    "execa": "^8.0.1",
    "lerna": "8.0.2",
    "lint-staged": "15.2.1",
    "node-fetch": "^3.3.2",
    "npm-run-all": "4.1.5",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "swagger2openapi": "^7.0.8",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "typescript": "5.3.3",
    "vitest": "^1.2.2"
  },
  "engines": {
    "pnpm": ">=8"
  },
  "publishConfig": {
    "access": "public"
  }
}
